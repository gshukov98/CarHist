@page "/cars"
<PageTitle>Cars</PageTitle>

<h1>Cars</h1>

<MatTable Items="@cars" class="mat-elevation-z5" PageSize="9999" ShowPaging="false">
    <MatTableHeader>
        <th style="width:20%">VIN</th>
        <th style="width:70%">Make</th>
    </MatTableHeader>
    <MatTableRow>
        <td>@context.VIN</td>
        <td>@context.Make</td>
    </MatTableRow>
</MatTable>
